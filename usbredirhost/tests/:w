use rusb;
use usbredirhost::{Device, DeviceHandler};

#[derive(Debug)]
struct Handler {}

impl DeviceHandler for Handler {
    fn log(&mut self) {
        dbg!("log");
    }
}

#[test]
fn test() {
    let ctxt = rusb::Context::new().unwrap();
    let handler = Handler {};
    let host = Device::new::<Context>(&ctxt, None, handler, 10);
    dbg!(host);
}
